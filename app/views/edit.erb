<form method='POST' action='/posts/<%= @post.id %>'>
  <input id="hidden" type="hidden" name="_method" value="patch">
  <input type="text" id="name" value="<%=@post.name%>" name="post[name]" >
  <input type="text" id="content" value="<%=@post.content%>" name="post[content]" >
  <input type="submit" value="submit">
</form>
